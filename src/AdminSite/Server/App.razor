@namespace SmallsOnline.Web.AdminSite.Server

<!DOCTYPE html>
<html lang="en">

<head>
    <base href="/" />

    <meta charset="utf-8" />

    <meta content="width=device-width, initial-scale=1.0, shrink-to-fit=no" name="viewport" />

    <PageTitle>Smalls.Online</PageTitle>
    <HeadOutlet @rendermode="@(new InteractiveServerRenderMode(false))" />

    <!-- Theme color attribute. Primarily for theming supported web browsers. -->
    <meta content="#496e9c" name="theme-color" />

    <!-- Twitter Posting Format -->
    <meta content="summary" name="twitter:card" />
    <meta content="@@thetimmysmalls" name="twitter:creator" />

    <!-- OpenGraph Data -->
    <meta content="https://smalls.online/smalls.png" property="og:image" />
    <meta property="og:locale" content="en_US" />

    <link href="favicon.ico" rel="icon" type="image/ico" />

    <link href="css/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="css/bootstrap-icons/bootstrap-icons.css" rel="stylesheet" />
    <link href="css/bootstrap-icons-custom.css" rel="stylesheet" />
    <link href="css/bootstrap-custom.css" rel="stylesheet" />
    <link href="css/app.css" rel="stylesheet" />
    <link href="css/animation/animation.css" rel="stylesheet" />
    <link href="highlight.js/styles/github.min.css" rel="stylesheet" />
    <link href="SmallsOnline.Web.PublicSite.Server.styles.css" rel="stylesheet" />

    <script src="highlight.js/highlight.min.js"></script>
</head>

<body>

    <Routes @rendermode="@(new InteractiveServerRenderMode(false))" />

    <script src="js/bootstrap/bootstrap.bundle.min.js"></script>
    <script src="_framework/blazor.web.js" suppress-error="BL9992"></script>
</body>

</html>