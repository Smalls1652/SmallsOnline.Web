    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>SmallsOnline.Web.Lib.Components</PackageId>
    <Authors>Timothy Small</Authors>
    <Company>Smalls.Online</Company>
